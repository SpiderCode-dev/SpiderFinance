   Description: Estructura de la tabla sfi_abonos
 -->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>importe</name>
        <type>double</type>
        <null>NO</null>
    </column>
    <column>
        <name>concept</name>
        <type>varchar(120)</type>
    </column>
    <column>
        <name>observations</name>
        <type>varchar(120)</type>
    </column>
    <column>
        <name>created_at</name>
        <type>datetime</type>
    </column>
    <column>
        <name>updated_at</name>
        <type>datetime</type>
    </column>
    <column>
        <name>codpago</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>iddocument</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>typedoc</name>
        <type>varchar(25)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>ca_sfi_abonos_formaspago</name>
        <type>FOREIGN KEY (codpago) REFERENCES formaspago (codpago) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_abonos_clientes</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>sfi_abonos_primary_key</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>