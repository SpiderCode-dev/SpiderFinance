<?xml version="1.0" encoding="UTF-8"?>

<view>
    <columns>
        <column name="code" order="100">
            <widget type="text" fieldname="code" />
        </column>
        <column name="zone" order="110">
            <widget type="autocomplete" fieldname="idzone" onclick="EditZona">
                <values source="sfi_zonas" fieldcode="id" fieldtitle="name"/>
            </widget>
        </column>
        <column name="Puertos" order="120">
            <widget type="text" fieldname="number_ports" />
        </column>
        <column name="Disponibles" order="130">
            <widget type="text" fieldname="available" />
        </column>
        <column name="coords" order="130">
            <widget type="text" fieldname="coords" />
        </column>
        <column name="observations" order="130">
            <widget type="text" fieldname="observations" />
        </column>
    </columns>
</view>