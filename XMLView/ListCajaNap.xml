<?xml version="1.0" encoding="UTF-8"?>

<view>
    <columns>
        <column name="name" order="100">
            <widget type="text" fieldname="name" />
        </column>
        <column name="zone" order="110">
            <widget type="autocomplete" fieldname="idzona" onclick="EditPlan">
                <values source="sfi_zonas" fieldcode="idzona" fieldtitle="name"/>
            </widget>
        </column>
        <column name="Distrito" order="120">
            <widget type="text" fieldname="distrito" />
        </column>
        <column name="Puertos" order="120">
            <widget type="text" fieldname="numero_puertos" />
        </column>
        <column name="Disponibles" order="130">
            <widget type="text" fieldname="disponibles" />
        </column>
        <column name="observations" order="130">
            <widget type="text" fieldname="observations" />
        </column>
    </columns>
</view>